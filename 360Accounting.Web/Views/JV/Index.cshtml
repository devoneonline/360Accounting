@Html.DevExpress().GetScripts(new Script { ExtensionSuite = ExtensionSuite.GridView })


@{
    ViewBag.Title = "Journal Voucher";
}

<div class="container">
    <div class="clearfix">
        <div class="col-lg-12">
            <h3 class="page-header">
                @ViewBag.Title
            </h3>
        </div>
    </div>
    <div class="col-lg-12">
        <div class="form-group">
            <input type="button" id="btnCreate" value="Create New" class="btn btn-default tstyCus-btn" />
        </div>
    </div>
    <div class="col-lg-12 cusGridWrapper">
        @Html.Action("JVPartial")
    </div>
</div>



<script type="text/javascript">
    $(document).ready(function () {
        $('#btnCreate').click(function () {
            window.location.href = '@Url.Content("~/JV/Create")' + '?sobId=4&periodId=6&currencyId=3';
        });
    });
</script>