@model _360Accounting.Web.Reports.AuditTrailReport

@Html.DevExpress().DocumentViewer(r =>
{
    r.Name = "DocumentViewer";
    r.Report = (XtraReport)Model;
    r.CallbackRouteValues = new 
    { 
        Controller = "JV",
        Action = "AuditTrailPartial",
        sobId = r.Report.Parameters["SOBId"].Value,
        fromDate = r.Report.Parameters["FromDate"].Value,
        toDate = r.Report.Parameters["ToDate"].Value
    };
    r.ExportRouteValues = new
    {
        Controller = "JV",
        Action = "AuditTrailPartialExport",
        sobId = r.Report.Parameters["SOBId"].Value,
        fromDate = r.Report.Parameters["FromDate"].Value,
        toDate = r.Report.Parameters["ToDate"].Value
    };
    r.ToolbarMode = DocumentViewerToolbarMode.StandardToolbar;
}).GetHtml()