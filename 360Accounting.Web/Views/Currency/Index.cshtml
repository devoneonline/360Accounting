@model _360Accounting.Web.Models.CurrencyListModel

@{
    ViewBag.Title = "Currencies";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            @ViewBag.Title
        </h1>
    </div>
</div>
<div class="form-group">
    <input type="button" id="btnCreate" value="Create New" class="btn btn-default" />
</div>
<div class="col-lg-12">
    @Html.Partial("_List", Model)
</div>
<script type="text/javascript">
    $(document).ready(function () {
        $('#btnCreate').click(function () {
            var url = '@Url.Content("~/Currency/Create")';
            window.location.href = url;
        });
    });
</script>

