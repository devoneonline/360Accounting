@Html.DevExpress().GetStyleSheets(new StyleSheet { ExtensionSuite = ExtensionSuite.GridView })
@Html.DevExpress().GetScripts(new Script { ExtensionSuite = ExtensionSuite.GridView })

@model _360Accounting.Web.Models.ReceivablePeriodListModel

@{
    ViewBag.Title = "Receivable Periods";
}

<div class="container">
    <div class="clearfix">
        <div class="col-lg-12">
            <h3 class="page-header">
                @ViewBag.Title
            </h3>
        </div>
    </div>

    <div class="clearfix">
        @if (ViewBag.ErrorMessage != "")
        {
            <div class="form-group col-lg-12">
                @ViewBag.ErrorMessage
            </div>
        }
        <div id="dvList" class="col-lg-12 cusGridWrapper">
            @Html.Action("ListPartial")
        </div>
    </div>
</div>
<script type="text/javascript">

</script>